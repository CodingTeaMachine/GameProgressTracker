{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "firefox",
			"request": "launch",
			"name": "Launch Firefix",
			"url": "http://gpt.local:80",
			"webRoot": "${workspaceFolder}",
			"timeout": 90000,
			"reAttach": true,
			"tmpDir": "/home/dekanymark/Apps/firefox-tmp",
			"pathMappings": [
				{
					"url": "http://gpt.local/src",
					"path": "${workspaceFolder}/src"
				}
			]
		}
	]
}
